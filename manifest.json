{
    "manifest_version": 2,
    "name": "和缓后台地址管理",
    "description": "后台地址太多了，不知道多少域名，先这样吧",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/logo3.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "notifications",
        "http://*/*",
        "https://*/*"
    ]
}